BUILD_TARGET = pcu
BOARD_NAME = pcu

COMMON_LIB_SRC :=  
CUBE_MAKEFILE_PATH = Cube-files

BUILD_DIR ?= ../Bin
EXTRA_LDFLAGS ?=

SRC_DIR := $(abspath Src)
SRC := $(wildcard $(SRC_DIR)/*.c)

$(info $(SRC))

INC_DIR := -I$(abspath Inc)

all:
	
	make -C $(CUBE_MAKEFILE_PATH) BUILD_DIR="$(BUILD_DIR)/$(BOARD_NAME)" EXTRA_C_SRC=$(SRC) EXTRA_C_INC=$(INC_DIR) TARGET=$(BUILD_TARGET) EXTRA_LDFLAGS="$(EXTRA_LDFLAGS)"